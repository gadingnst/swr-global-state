{
  "name": "@gadingnst/store-swr",
  "author": "Sutan Gading Fadhillah Nasution <contact@gading.dev>",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/use-store-swr.esm.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@gadingnst"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepare": "tsdx build",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsc --noEmit && eslint src --max-warnings 0",
    "lint:fix": "tsc --noEmit && eslint src --fix",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/use-store-swr.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-store-swr.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "react": ">=18",
    "swr": ">=1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  }
}
